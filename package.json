    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.2",
    "nodemon": "^1.19.0"
  }
}
